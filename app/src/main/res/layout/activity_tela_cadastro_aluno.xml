<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".aluno.TelaCadastro_Aluno">

  <com.example.unigym.ReturnView
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      app:returnActivity="com.example.unigym.TelaInicial" />
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:gravity="center"
      android:orientation="vertical"
      tools:layout_editor_absoluteX="1dp"
      tools:layout_editor_absoluteY="1dp">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="159dp"
        android:layout_height="159dp"
        android:layout_marginBottom="50dp"
        app:srcCompat="@drawable/image_2" />

    <EditText
        android:id="@+id/Digitar_Nome_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Digite seu Nome" />

    <EditText
        android:id="@+id/Digitar_DataNiv_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Digite seu Aniversario" />

    <EditText
        android:id="@+id/Digitar_email_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="E-mail" />

    <EditText
        android:id="@+id/Digitar_senha_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Senha" />

    <EditText
        android:id="@+id/Digitar_sexo_prof"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Sexo (opcional)" />

    <Button
        android:id="@+id/Criar_conta_prof"
        android:layout_width="163dp"
        android:layout_height="51dp"
        android:backgroundTint="#3F51B5"
        android:text="Criar Conta" />
  </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>